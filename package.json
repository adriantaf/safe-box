{
  "author": "Adrian Tafoya Morales <atafoya863@gmail.com>",
  "description": "App para gestionar contrase√±as",
  "name": "safe-box",
  "private": true,
  "version": "1.1.2",
  "main": "electron/main.js",
  "icon": "assets/icons/safe-box.png",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.example.safe-box",
    "productName": "Safe box",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "output": "dist-electron"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icons/safe-box.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "assets/icons/linux/"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icons/safe-box.ico"
    }
  },
  "dependencies": {
    "better-sqlite3": "11.8.1",
    "crypto": "^1.0.1",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.19.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@vitejs/plugin-react-swc": "3.7.2",
    "autoprefixer": "^10.4.20",
    "electron": "34.0.1",
    "electron-builder": "25.1.8",
    "eslint": "9.19.0",
    "eslint-plugin-react": "7.37.4",
    "globals": "15.14.0",
    "tailwindcss": "^4.0.0",
    "vite": "6.0.11",
    "vite-plugin-electron": "0.29.0"
  }
}
