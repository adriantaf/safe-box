{
  "author": "Adrian Tafoya Morales",
  "description": "App para gestionar contrase√±as",
  "name": "safe-box",
  "private": true,
  "version": "0.0.0",
  "main": "electron/main.js",
  "icon": "assets/icons/safe-box.png",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.example.safe-box",
    "productName": "Safe box",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "output": "dist-electron"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icons/safe-box.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "assets/icons/"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icons/safe-box.ico"
    }
  },
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "better-sqlite3": "^11.7.2",
    "crypto": "^1.0.1",
    "postcss": "^8.4.49",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "tailwindcss": "^3.4.17",
    "vite": "^6.0.5",
    "vite-plugin-electron": "^0.29.0"
  }
}
